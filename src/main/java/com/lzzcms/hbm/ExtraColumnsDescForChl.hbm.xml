<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lzzcms.model">
    <class name="ExtraColumnsDescForChl" table="lzz_extracolumnsdescforchl" >
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="colName" type="string" length="30" column="colname"/>
        <property name="colType" type="string" length="30" column="coltype"/>
        <property name="showTip" type="string" length="30" column="showtip"/>
        <property name="allowNull" type="boolean" column="allownull"/>
        <property name="defaultVal" type="string" length="30" column="defaultval"/>
        <property name="additionalTable" type="string" length="30" column="additionaltable"/>
    </class>
</hibernate-mapping>

