<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lzzcms.model">
    <class name="SystemParam" table="lzz_systemparam"  dynamic-update="true" dynamic-insert="true">
        <id name="id" column="id">
            <generator class="native">
            </generator>
        </id>
        <property name="groupName" type="string" length="150" column="groupname"/>
        <property name="paramName" type="string" length="150" column="paramname"/>
        <property name="paramValue" type="string" length="600"  column="paramvalue"/>
    </class>
</hibernate-mapping>

